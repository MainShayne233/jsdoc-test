{
  "name": "jsdoc-test",
  "version": "0.0.3",
  "description": "Test JSDoc style doc examples in your test suite",
  "entry": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --watch-stdin --colors",
    "build": "NODE_ENV=production ./node_modules/.bin/webpack -p --config webpack.config.js",
    "test": "./node_modules/.bin/mocha --require babel-register",
    "test:watch": "./node_modules/.bin/mocha --require babel-register --watch"
  },
  "keywords": [
    "jsdoc",
    "test",
    "doctest",
    "doc",
    "docs",
    "chai",
    "elixir"
  ],
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MainShayne233/jsdoc-test.git"
  },
  "author": "MainShayen233",
  "license": "MIT",
  "dependencies": {
    "chai": "^3.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-jest": "^20.0.1",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-remove-strict-mode": "0.0.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "mocha": "^3.4.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
